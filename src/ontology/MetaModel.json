{
  "type": "pt",
  "classifiers": [
    {
      "type": "place",
      "labels": [
        "name",
        "marking"
      ]
    },
    {
      "type": "transition",
      "labels": [
        "name"
      ]
    }
  ],
  "relations": [
    {
      "type": "arc",
      "bind": {
        "place": [
          "transition"
        ],
        "transition": [
          "place"
        ]
      },
      "arrow-end": "arrow-head",
      "labels": [
        "name"
      ]
    }
  ],
  "arrow-heads": [
    {
      "type": "arrow-head"
    }
  ],
  "texts": [
    {
      "type": "name"
    },
    {
      "type": "marking",
      "regex": "\\d+",
      "default": "1"
    }
  ]
}

